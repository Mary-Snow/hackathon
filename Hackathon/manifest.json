{
  "manifest_version": 2,
  "name": "My Cool Extension",
  "version": "0.1",
  "background": {
    "scripts": ["background.js"], 
      "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-3.1.1.slim.js","content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "link.png"
  }, 
  "permissions":["storage"]
  }
